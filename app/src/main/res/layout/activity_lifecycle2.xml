<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.ware.com.ccino.ware.jetpack.LifecycleActivity">


    <TextView
        android:id="@+id/mDataView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="显示数据处22" />

    <Button
        android:layout_width="wrap_content"
        android:id="@+id/mOpView"
        android:layout_height="wrap_content"
        android:text="Fragment 操作"
        app:layout_constraintTop_toBottomOf="@id/mDataView" />

    <FrameLayout
        app:layout_constraintTop_toBottomOf="@id/mOpView"
        android:id="@+id/mCoroutineModeView"
        android:layout_width="match_parent"
        android:layout_height="100dp"/>


</androidx.constraintlayout.widget.ConstraintLayout>