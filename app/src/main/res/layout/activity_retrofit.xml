<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.ccino.ware.retrofit.RetrofitActivity"
    tools:ignore="MissingConstraints">

    <Button
        android:id="@+id/commonBt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="普通用法" />

    <Button
        android:id="@+id/rxBt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:text="集rx"
        app:layout_constraintLeft_toRightOf="@id/commonBt" />

    <TextView
        android:id="@+id/coroutineBt"
        style="@style/DefaultTv"
        android:text="集协程"
        app:layout_constraintLeft_toRightOf="@id/rxBt" />


</androidx.constraintlayout.widget.ConstraintLayout>