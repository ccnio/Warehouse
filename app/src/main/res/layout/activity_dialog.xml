<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="MissingConstraints">

    <TextView
        android:id="@+id/commonView"
        style="@style/DefaultTv"
        android:text="普通" />

    <TextView
        android:id="@+id/dialogBuilderView"
        style="@style/DefaultTv"
        android:text="Dialog builder"
        app:layout_constraintLeft_toRightOf="@id/commonView" />

    <TextView
        android:id="@+id/mDialogView"
        style="@style/DefaultTv"
        android:text="Dialog常规"
        app:layout_constraintLeft_toRightOf="@id/dialogBuilderView" />

    <TextView
        android:id="@+id/extendView"
        style="@style/DefaultTv"
        android:text="Dialog继承"
        app:layout_constraintLeft_toRightOf="@id/mDialogView" />
</androidx.constraintlayout.widget.ConstraintLayout>
